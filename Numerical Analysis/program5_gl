function I=GL(a)
x=[0.9061798459,-0.9061798459,0.5384693101,-0.5384693101,0];
A=[0.2369268851,0.2369268851,0.4786286705,0.4786286705,0.5688888889];
%xx= 0.5*a+0.25*x;
xx= 0.25*a+0.125*x;
for i=1:5
%t(1,i)=0.25*A(1,i)*(1/(xx(1,i)^2))*sin(2*pi/xx(1,i));
t(1,i)=0.125*A(1,i)*(1/(xx(1,i)^2))*sin(2*pi/xx(1,i));

end
I=sum(sum(t));
end

